{
  "semi": false,
  "printWidth": 80,
  "singleQuote": false,
  "bracketSpacing": true,
  "trailingComma": "none",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "jsxBracketSameLine": false,
  "importOrderSeparation": true,
  "embeddedLanguageFormatting": "auto",
  "importOrder": ["^react$", "^@?\\w", "^[./]"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
